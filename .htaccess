<IfModule mod_rewrite.c>
    RewriteEngine On
    
    RewriteCond %{REQUEST_URI} ^/assets/
    RewriteCond %{REMOTE_ADDR} !^127\.0\.0\.1$   
    RewriteCond %{REMOTE_ADDR} !^::1$               
    RewriteRule ^ - [R=403,L] 

    # Error page for 403 and 404
    ErrorDocument 403 /404.php
    ErrorDocument 404 /404.php
</IfModule>
